{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "gdb",
      "request": "attach",
      "name": "Wokwi Debug",
      "executable":
      "${workspaceFolder}/target/target.xtensa-esp8266-none-elf/debug/esp8266_serial_mqtt",
      "target": "localhost:9333",
      "remote": true,
      "gdbpath":
      "xtensa-esp8266-elf/bin/xtensa-esp8266-elf-gdb",
      "cwd": "${workspaceRoot}",
      "stopAtConnect": true,
      "valuesFormatting": "parseText"
    }
  ]
}